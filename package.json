{
  "name": "@venice/ai-sdk-provider",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "pack": "rm -rf dist && tsup && npm pack",
    "lint": "eslint \"./**/*.ts*\"",
    "lint-fix": "eslint \"./**/*.ts*\" --fix",
    "type-check": "tsc --noEmit",
    "prettier-check": "prettier --check \"./**/*.ts*\"",
    "prettier-fix": "prettier --write \"./**/*.ts*\"",
    "test": "pnpm test:node && pnpm test:edge",
    "test:node": "vitest --config vitest.node.config.ts --run",
    "test:edge": "vitest --config vitest.edge.config.ts --run",
    "test:watch": "vitest --config vitest.node.config.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@ai-sdk/test-server": "^1.0.1",
    "@edge-runtime/vm": "^5.0.0",
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.9",
    "eslint": "^9.39.2",
    "prettier": "^3.8.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17",
    "zod": "^4.3.5"
  },
  "peerDependencies": {
    "ai": "^6.0.38",
    "zod": "^3.25.0 || ^4.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/dpuyosa/venice-ai-sdk-provider",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dpuyosa/venice-ai-sdk-provider.git"
  },
  "bugs": {
    "url": "https://github.com/dpuyosa/venice-ai-sdk-provider/issues"
  },
  "keywords": [
    "ai",
    "venice",
    "venice-ai",
    "vercel",
    "vercel-ai-sdk",
    "openai-compatible",
    "agent",
    "private"
  ]
}
